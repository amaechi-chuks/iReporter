{
    "extends": "airbnb-base",
    
    "env": {
        "mocha": true,
        "browser": true,
        "es6":true,
        "node":true
    },

"rules": {
		"one-var": 0,
		"import/no-named-as-default": 0,
		"one-var-declaration-per-line": 0,
		"new-cap": 0,
		"no-tabs": 0,
		"consistent-return": 0,
		"no-param-reassign": 0,
		"comma-dangle": 0,
		"curly": ["error", "multi-line"],
		"import/no-unresolved": [2, { "commonjs": true }]
	},
	"settings": {
		"import/resolver": "webpack"
	}
    
}