{
    "extends": "airbnb-base",
    
    "env": {
        "mocha": true,
        "browser": true,
        "es6":true,
        "node":true
    },
    "rules": {
        "import/prefer-default-export": "off",
        "comma-dangle": 0,
        "curly": ["error", "multi-line"],
        "import/no-unresolved": [2, { "commonjs": true }],
        "no-shadow": ["error", { "allow": ["req", "res", "err"] }]
        
    }
}